  "configurations": [
    {
      "name": "Make and debug in plugin host",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Builds/LinuxMakefile/build/${config:juce.project_name}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
